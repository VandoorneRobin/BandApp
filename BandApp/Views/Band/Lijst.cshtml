
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Lijst</title>
</head>

<body>
    <h2>Bands</h2>

    <ol>
        @foreach (var band in ViewBag.bands)
        {
            <li>@band.Naam - (@band.Jaar)</li>
        }
    </ol>

    @*@Html.ActionLink("Naar Lijst met leden", "LijstMetLeden)*@
    <a asp-action="LijstMetLeden">Naar Lijst met de leden</a>
</body>

</html>

